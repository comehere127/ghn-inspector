{
  "__chrome|edge|opera__manifest_version": 3,
  "__firefox__manifest_version": 2,
  "name": "GHN Inspector",
  "version": "1.0.1",
  "icons": {
    "32": "assets/icons/favicon-32.png",
    "128": "assets/icons/favicon-128.png",
    "180": "assets/icons/favicon-180.png",
    "192": "assets/icons/favicon-192.png"
  },
  "description": "MFE Inspector for local frontend development of GHN SingleWeb",
  "homepage_url": "https://github.com/comehere127/ghn-inspector",
  "short_name": "MFE Inspector",
  "__chrome|edge|opera__permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "__firefox__permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "https://*.ghn.tech/*",
    "http://localhost/*"
  ],
  "__chrome|edge|opera__host_permissions": [
    "https://*.ghn.tech/*",
    "http://localhost/*"
  ],
  "__firefox__content_security_policy": "script-src 'self'; object-src 'self'",
  "__chrome|edge|opera__content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "author": "TuDHM",
  "__firefox__browser_specific_settings": {
    "gecko": {
      "id": "{f7adb136-f50e-4948-a135-55c73ba393d1}"
    }
  },
  "__chrome__minimum_chrome_version": "88",
  "__opera__minimum_opera_version": "36",
  "devtools_page": "devtools.html"
}
